{
  "name": "knowledge",
  "version": "1.0.0",
  "repository": "https://github.com/ViktorKugay/knowledge",
  "scripts": {
    "format": "prettier --write --config .prettierrc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-checkout": "yarnhook",
      "post-merge": "yarnhook",
      "post-rewrite": "yarnhook"
    }
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --config .prettierrc",
      "git add"
    ]
  },
  "author": "Viktor Kugay <v.kugay@tinkoff.ru>",
  "license": "MIT",
  "dependencies": {
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "prettier": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2",
    "yarnhook": "^0.4.2"
  }
}
